<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <appSettings>
    <add key="Disco.Endpoint" value="http://localhost/soa/broobu/disco" />
    <add key="Cloud.Probe" value="http://localhost:1800/cloud/probe"/>
    <add key="Cloud.Announce" value="http://localhost:1800/cloud/announce"/>
    <add key="Service.CommonName" value="Broobu Discovery Monitor Service" />
    <add key="Binding.ReceiveTimeout" value="01:00:00" />
    <add key="Binding.SendTimeout" value="01:00:00" />
    <add key="Service.AutoRegisterDomain" value="false" />
    <add key="Service.EnableMex" value="true" />
    <add key="Service.SecureMex" value="true" />
    <add key="Service.Register" value="true" />
    <add key="Service.AnnounceDelay" value="5" />
    <add key="Service.IsRestful" value="true" />
  </appSettings>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="NLog" publicKeyToken="5120e14c03d0593c" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-3.0.0.0" newVersion="3.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
